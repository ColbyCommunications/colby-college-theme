    "scripts": {
        "tailwind:build": "tailwind build style.css -o dist/styles/tailwind.css",
        "tailwind:watch": "tailwindcss -i style.css -o dist/styles/tailwind.css  --watch",
        "fractal": "fractal start --sync",
        "fractal:build": "fractal build",
        "scripts:build": "webpack --config ./config/webpack/webpack.prod.js",
        "scripts:watch": "webpack --watch --config ./config/webpack/webpack.dev.js",
        "components:watch": "concurrently \"npm:fractal\" \"npm:scripts:watch\" \"npm:tailwind:watch\"",
        "components:build": "run-s scripts:build fractal:build",
        "build": "npm-run-all scripts:build components:build tailwind:build",
        "build:platform": "npm-run-all scripts:build tailwind:build",
        "watch": "npm run components:watch"
    },
